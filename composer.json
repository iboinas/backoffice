{
    "name": "iboinas/backoffice",
    "description": "Backoffice with bootstrap for Cartalyst/Sentinel",
    "homepage": "https://github.com/thephpiboinas/backoffice",
    "license": "MIT",
    "authors": [
        {
            "name": "Joao Patricio",
            "email": "iboinas@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0",
        "cartalyst/sentinel": "2.0.14",
        "cocur/slugify": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Iboinas\\Backoffice\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Iboinas\\Backoffice\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
